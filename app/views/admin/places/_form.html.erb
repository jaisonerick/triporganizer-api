<%= simple_form_for([:admin, @place]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :google_places_id %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :picture_url %>
    <%= f.input :address %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>
