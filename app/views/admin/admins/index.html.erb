<p id="notice"><%= notice %></p>

<h1>Admin Admins</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.email %></td>
        <td><%= admin.name %></td>
        <td><%= link_to 'Show', [:admin, admin] %></td>
        <td><%= link_to 'Edit', edit_admin_admin_path(admin) %></td>
        <td><%= link_to 'Destroy', [:admin, admin], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Admin', new_admin_admin_path %>
