<div class="row">
  <div class="col-md-12">
    <div class="page-header">
      <div class="btn-group pull-right">
        <%= link_to 'Voltar', admin_companies_path, class: 'btn btn-default' %>
        <%= link_to 'Editar', edit_admin_company_path(@company), class: 'btn btn-default' %>
      </div>

      <h1>Visualizando Empresa</h1>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Nome:</dt>
      <dd><%= @company.name %></dd>

      <dt>Descrição:</dt>
      <dd><%= @company.description %></dd>

      <dt>Endereço:</dt>
      <dd><%= @company.address %></dd>

      <dt>Telefone:</dt>
      <dd><%= @company.phone %></dd>

      <dt>Tipo:</dt>
      <dd><%= @company.company_type %></dd>

    </dl>
  </div>
</div>
