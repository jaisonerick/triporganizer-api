<%= simple_form_for([:admin, @admin_user]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
    <%= f.input :name %>
    <%= f.input :picture_url %>
    <%= f.input :cpf %>
    <%= f.input :birthday %>
    <%= f.input :address %>
    <%= f.input :phone %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
